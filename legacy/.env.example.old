# SuperAgent n8n Environment Configuration

# Discord Bot Tokens (from SuperAgent registry)
DISCORD_TOKEN_GROK4=MTEyMzQ1Njc4OTAxMjM0NTY3OA.EXAMPLE.dummyTokenForSuperAgentGrok4Bot123456
DISCORD_TOKEN_CLAUDE=MTEyMzQ1Njc4OTAxMjM0NTY3OA.EXAMPLE.dummyTokenForCryptoTaxCoderDev1Bot123456
DISCORD_TOKEN_GEMINI=MTEyMzQ1Njc4OTAxMjM0NTY2OA.EXAMPLE.dummyTokenForSuperAgentGeminiBot123456

# Discord Configuration
DEFAULT_SERVER_ID=1395578178973597799
GENERAL_CHANNEL_ID=1395578179531309089
DISCORD_BOT_USER_ID=1396750004588253205

# LLM API Keys
XAI_API_KEY=your_xai_grok_api_key_here
ANTHROPIC_API_KEY=your_anthropic_claude_api_key_here
GOOGLE_AI_API_KEY=your_google_gemini_api_key_here

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=superagent_n8n
POSTGRES_USER=superagent
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis Configuration  
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here

# n8n Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=your_n8n_admin_password_here
N8N_ENCRYPTION_KEY=your_32_character_encryption_key_here

# Discord Webhook URLs (generated by n8n)
N8N_WEBHOOK_BASE_URL=http://localhost:5678/webhook
DISCORD_WEBHOOK_URL=${N8N_WEBHOOK_BASE_URL}/discord

# Application Settings
NODE_ENV=development
LOG_LEVEL=info
MAX_CONCURRENT_WORKFLOWS=10

# Security Settings
ENABLE_RATE_LIMITING=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090
HEALTH_CHECK_PORT=8080

# Feature Flags
ENABLE_MEMORY_SYSTEM=true
ENABLE_TOOL_CALLING=true
ENABLE_MULTI_AGENT_ROUTING=true
ENABLE_CONVERSATION_THREADING=true